<h1><%= @coffee.name %></h1>
<h2>Origin: </h2><h3><%= @coffee.origin %></h3>
<h2>Variety: </h2><h3><%= @coffee.variety %></h3>
<h2>Notes: </h2><h3><%= @coffee.notes %></h3>
<h2>Intensifiers: </h2><h3><%= @coffee.intensifier %></h3>
<h3>Created By: </h3><h4><%= @coffee.user.name %></h4>
<h4>Created At: </h4><h5><%= @coffee.created_at %></h5>
<p><a href="/coffees/<%= @coffee.id %>/edit">Edit</a></p>
<p>
<form action="/coffees/<%=@coffee.id%>/delete" method="POST">
  <input type="hidden" name="_method" value="delete">
  <input type="submit" value="Delete">
</form>
</p>
